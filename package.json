{
  "name": "streamify-core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "easy": "pnpm i && pnpm build && pnpm format && pnpm dev",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,js,tsx,css,json}\"",
    "format:check": "prettier --check \"**/*.{ts,js,tsx,css,json}\"",
    "ui:add": "pnpm --filter ui ui:add"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.15",
    "eslint": "^8.51.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.0.3",
    "turbo": "^1.10.15"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.6.12",
    "yarn": ">=1.22.0 < 2.0.0"
  }
}
